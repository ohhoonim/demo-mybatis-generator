<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "https://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
  <context id="dsql" targetRuntime="MyBatis3DynamicSql">
    <plugin type="org.mybatis.generator.plugins.UnmergeableXmlMappersPlugin"/>
    <plugin type="org.mybatis.generator.plugins.dsql.ReadOnlyPlugin"/>
    <commentGenerator>
      <property name="suppressAllComments" value="true" />
      <property name="addRemarkComments" value="true" />
    </commentGenerator>
    <jdbcConnection driverClass="org.postgresql.Driver"
      connectionURL="jdbc:postgresql://localhost:15445/mydatabase"
      userId="myuser" password="secret" />
    <javaModelGenerator targetPackage="menu.model" targetProject="../src" />
    <javaClientGenerator targetPackage="menu.mapper" targetProject="../src" />
    <table tableName="system_domain_menu_attribute_tag" />
    <table tableName="system_domain_menu_item" />
    <table tableName="system_domain_menu_text" />
  </context>
  <context id="xsql" targetRuntime="MyBatis3">
    <commentGenerator>
      <property name="suppressAllComments" value="true" />
    </commentGenerator>
    <jdbcConnection driverClass="org.postgresql.Driver"
      connectionURL="jdbc:postgresql://localhost:15445/mydatabase"
      userId="myuser" password="secret" />

    <javaModelGenerator targetPackage="menu.model" targetProject="../src" />
    <sqlMapGenerator targetPackage="menu.xml" targetProject="../src" />
    <!-- <javaClientGenerator type="XMLMAPPER" targetPackage="menu.dao" targetProject="../src" /> -->

    <table tableName="system_domain_menu_attribute_tag" />
    <table tableName="system_domain_menu_item" />
    <table tableName="system_domain_menu_text" />
  </context>
</generatorConfiguration>